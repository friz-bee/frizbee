{
  "name": "frizbee",
  "version": "0.0.8",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 20.0.0",
    "pnpm": ">= 8.0.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write --list-different .",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "tag:patch": "pnpm version patch && git push --follow-tags",
    "tag:minor": "pnpm version minor && git push --follow-tags",
    "tag:major": "pnpm version major && git push --follow-tags",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.35",
    "@iconify-json/openmoji": "^1.2.8",
    "@iconify-json/simple-icons": "^1.2.31",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/ui": "^3.0.2",
    "@nuxtjs/i18n": "9.5.3",
    "@types/node": "^22.14.0",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "nuxt": "^3.16.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "yup": "^1.6.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "cypress": "^14.3.0",
    "mochawesome": "^7.1.3"
  }
}
